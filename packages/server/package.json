    "graphql": "^16.8.1",
    "graphql-yoga": "3.9.1",
    "mongodb": "^5.9.1"
  },
  "devDependencies": {
    "@types/node": "^18.16.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
