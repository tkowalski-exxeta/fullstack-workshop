{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm:build:watch\" \"npm:codegen:watch\"",
    "build:watch": "cross-env NODE_ENV=development ts-node-dev --exit-child --respawn src/main.ts",
    "start": "ts-node src/main.ts",
    "codegen": "graphql-codegen",
    "codegen:watch": "graphql-codegen --watch",
    "prestart": "npm run codegen"
  },
